#include <stdio.h>
int main()
{
    int n,count=0;
    double arr[100];
    count++;
    printf("Enter the number of elements");
    count++;
    scanf("%d", &n);
    count++;
    for (int i=0;i<n;++i)
    {
  	    count++;
  	    count++;
        printf("Enter number%d: ", i + 1);
        count++;
        scanf("%lf", &arr[i]);
        count++;
    }
    count++;
    for(int i=1;i<n;++i)
    {
  	    count++;
  	    count++;
        if(arr[0]<arr[i])
	    {
            arr[0] = arr[i];
        }
        count++;
        count++;
  }
  count++;
  printf("Largest element = %.2lf  ", arr[0]);
  count++;
  printf("time complexity is %d",count);
  return 0;
}
